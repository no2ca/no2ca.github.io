---
export interface Props {
    links: { name: string; url: string }[];
}
const { links } = Astro.props;
---
<section>
    <h2>Links</h2>
    <ul>
        {links.map(link => (
            <li>
                <a href={link.url} target="_blank">{link.name}</a>
            </li>
        ))}
    </ul>
</section>
		
<style>
    ul {
        list-style: none;
        padding: 0;
    }
</style>